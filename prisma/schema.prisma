

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String
  password String
}

model Profile {
  id        String @id @default(uuid())
  name      String
  email     String
  phone     String
}


